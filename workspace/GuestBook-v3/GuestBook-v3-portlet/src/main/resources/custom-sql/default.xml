<custom-sql>
    <sql id="com.liferay.docs.guestbook.service.persistence.GuestbookFinder.buscarPorNombre">
		<![CDATA[
		        SELECT GB_Guestbook.*
		        FROM GB_Guestbook
		        WHERE
		            GB_Guestbook.name LIKE ?
		]]>
    </sql>
</custom-sql>